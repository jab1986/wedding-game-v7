config_version=5

[application]

config/name="Mark & Jenny's Wedding Adventure"
config/description="A disaster-filled SNES-style wedding adventure"
config/version="1.0.0"
run/main_scene="res://scenes/ui/MainMenu.tscn"
config/features=PackedStringArray("4.4")
config/icon="res://icon.svg"

[autoload]

GameManager="*res://scripts/autoload/GameManager.gd"
AudioManager="*res://scripts/autoload/AudioManager.gd"
SceneTransition="*res://scenes/autoload/SceneTransition.tscn"
SpriteManager="*res://scripts/utils/SpriteManager.gd"

[display]

window/size/viewport_width=768
window/size/viewport_height=672
window/size/mode=2
window/stretch/mode="canvas_items"

[editor]

naming/scene_name_casing=1

[editor_plugins]

enabled=PackedStringArray("res://addons/dialogue_manager/plugin.cfg", "res://addons/beehave/plugin.cfg", "res://addons/phantom_camera/plugin.cfg", "res://addons/wyvernbox/editor/plugin.cfg")

